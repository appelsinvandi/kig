{
  "name": "kig",
  "version": "0.3.1",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Appelsinvandi/kig"
  },
  "files": [
    "lib/",
    "esm/"
  ],
  "bin": {
    "kig": "./lib/bin/index.js"
  },
  "scripts": {
    "start": "ts-node --project tsconfig.dev.json -r tsconfig-paths/register ./src/bin/index.ts",
    "build:cjs": "tsc -p ./tsconfig.json && tscpaths --project ./tsconfig.dev.json -s ./src -o ./lib",
    "build:es": "tsc -p ./tsconfig.json -m esNext --outDir esm && tscpaths --project ./tsconfig.dev.json -s ./src -o ./esm",
    "build": "yarn cleanup && yarn build:cjs && yarn build:es",
    "cleanup": "rimraf lib/**/* && rimraf esm/**/*",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "arrify": "^2.0.1",
    "await-to-js": "^2.1.1",
    "chalk": "^4.1.0",
    "change-case": "^4.1.1",
    "cosmiconfig": "^7.0.0",
    "ejs": "^3.1.5",
    "enquirer": "^2.3.6",
    "joi": "^17.2.1",
    "js-yaml": "^3.14.0"
  },
  "devDependencies": {
    "@types/ejs": "3.0.7",
    "@types/js-yaml": "3.12.10",
    "@types/node": "12.20.55",
    "husky": "4.3.8",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.3",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "4.2.4"
  },
  "keywords": [
    "generate",
    "boilerplate",
    "scaffold",
    "template",
    "cli"
  ]
}
